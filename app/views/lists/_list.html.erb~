<li>
  <span class="name"><%= list.name %></span>
  <%= link_to list.name, list %>
  <span class="timestamp">
    Posted <%= time_ago_in_words(list.created_at) %> ago.
  </span>
  <% if current_user?(list.user) %>
    <%= link_to "delete", list, method: :delete,
                                     data: { confirm: "You sure?" },
                                     title: list.name %>
  <% end %>
</li>

